{
  "manifest_version": 3,
  "name": "Sticky Notes 1.0",
  "version": "1.0",
  "description": "A simple and beautiful note-taking extension.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icons/icon16.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  }
}
